{
  "scope": "environment",
  "disk": {
    "diskLowInodesDetection": {
      "customThresholds": {
        "freeInodesPercentage": 5
      },
      "detectionMode": "custom",
      "enabled": true
    },
    "diskLowSpaceDetection": {
      "customThresholds": {
        "freeSpacePercentage": 3
      },
      "detectionMode": "custom",
      "enabled": true
    },
    "diskSlowWritesAndReadsDetection": {
      "customThresholds": {
        "writeAndReadTime": 200
      },
      "detectionMode": "custom",
      "enabled": true
    }
  }
}